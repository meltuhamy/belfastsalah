<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.meltuhamy.londonsalah" version="2.0.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Prayer Times</name>
  <description>Prayer times with notifications</description>
  <author email="tuhamy@gmail.com" href="http://meltuhamy.com/">Mohamed Eltuhamy</author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon qualifier="ldpi" src="resources/res/icons/android/icon-36-ldpi.png"/>
    <icon qualifier="mdpi" src="resources/res/icons/android/icon-48-mdpi.png"/>
    <icon qualifier="hdpi" src="resources/res/icons/android/icon-72-hdpi.png"/>
    <icon qualifier="xhdpi" src="resources/res/icons/android/icon-96-xhdpi.png"/>
    <icon qualifier="xxhdpi" src="resources/res/icons/android/icon-144-xxhdpi.png"/>
    <icon qualifier="xxxhdpi" src="resources/res/icons/android/icon-192-xxxhdpi.png"/>
    <splash qualifier="ldpi" src="resources/res/screens/android/screen-ldpi-portrait.png"/>
    <splash qualifier="mdpi" src="resources/res/screens/android/screen-mdpi-portrait.png"/>
    <splash qualifier="hdpi" src="resources/res/screens/android/screen-hdpi-portrait.png"/>
    <splash qualifier="xhdpi" src="resources/res/screens/android/screen-xhdpi-portrait.png"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <icon src="resources/res/icons/ios/icon-small.png" width="29" height="29"/>
    <icon src="resources/res/icons/ios/icon-small-2x.png" width="58" height="58"/>
    <icon src="resources/res/icons/ios/icon-40.png" width="40" height="40"/>
    <icon src="resources/res/icons/ios/icon-40-2x.png" width="80" height="80"/>
    <icon src="resources/res/icons/ios/icon-50.png" width="50" height="50"/>
    <icon src="resources/res/icons/ios/icon-50-2x.png" width="100" height="100"/>
    <icon src="resources/res/icons/ios/icon.png" width="57" height="57"/>
    <icon src="resources/res/icons/ios/icon-2x.png" width="114" height="114"/>
    <icon src="resources/res/icons/ios/icon-60.png" width="60" height="60"/>
    <icon src="resources/res/icons/ios/icon-60-2x.png" width="120" height="120"/>
    <icon src="resources/res/icons/ios/icon-60-3x.png" width="180" height="180"/>
    <icon src="resources/res/icons/ios/icon-72.png" width="72" height="72"/>
    <icon src="resources/res/icons/ios/icon-72-2x.png" width="144" height="144"/>
    <icon src="resources/res/icons/ios/icon-76.png" width="76" height="76"/>
    <icon src="resources/res/icons/ios/icon-76-2x.png" width="152" height="152"/>
    <splash src="resources/res/screens/ios/screen-iphone-portrait.png" width="320" height="480"/>
    <splash src="resources/res/screens/ios/screen-iphone-portrait-2x.png" width="640" height="960"/>
    <splash src="resources/res/screens/ios/screen-iphone-portrait-568h-2x.png" width="640" height="1136"/>
    <splash src="resources/res/screens/ios/screen-iphone-portrait-667h.png" width="750" height="1334"/>
    <splash src="resources/res/screens/ios/screen-iphone-portrait-736h.png" width="1242" height="2208"/>
    <splash src="resources/res/screens/ios/screen-ipad-portrait.png" width="768" height="1024"/>
    <splash src="resources/res/screens/ios/screen-ipad-portrait-2x.png" width="1536" height="2048"/>
  </platform>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashMaintainAspectRatio" value="true"/>
  <preference name="FadeSplashScreenDuration" value="300"/>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="cordova-plugin-whitelist" spec="1.3.1"/>
  <plugin name="cordova-plugin-console" spec="1.0.5"/>
  <plugin name="cordova-plugin-statusbar" spec="2.2.1"/>
  <plugin name="cordova-plugin-device" spec="1.1.4"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.1"/>
  <plugin name="cordova-sqlite-storage" spec="~2.0.0"/>
  <plugin name="ionic-plugin-deploy" spec="~0.6.6"/>
  <plugin name="de.appplant.cordova.plugin.local-notification" spec="https://github.com/Telerik-Verified-Plugins/LocalNotification"/>
  <allow-navigation href="http://192.168.1.2:8100"/>
  <allow-navigation href="http://192.168.0.6:8100"/>
</widget>
